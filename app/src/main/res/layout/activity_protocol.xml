<androidx.constraintlayout.widget.ConstraintLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:background="@color/white"
    tools:context=".activities.ProtocolActivity">

    <TextView
        android:id="@+id/protocolselect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Protocol Select"
        android:textColor="@color/gnt_black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/btnCross"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="14dp"
        android:layout_marginTop="12dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_close_24" />

    <androidx.appcompat.widget.AppCompatButton
        android:background="@drawable/bg"
        android:elevation="10dp"
        android:id="@+id/tls"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_width="wrap_content"
        android:paddingBottom="15dp"
        android:paddingLeft="10dp"
        android:paddingRight="80dp"
        android:paddingTop="15dp"
        android:text="STP"
        android:textAlignment="textStart"
        android:textAllCaps="true"
        android:textColor="@color/gnt_black"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tpl" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bgg"
        android:elevation="10dp"
        android:paddingLeft="5dp"
        android:paddingTop="2dp"
        android:paddingRight="5dp"
        android:paddingBottom="2dp"
        android:text="Developing"
        android:textAlignment="textStart"
        android:textColor="@color/gnt_black"
        android:textSize="8sp"
        app:layout_constraintBottom_toBottomOf="@+id/tls"
        app:layout_constraintEnd_toEndOf="@+id/tls"
        app:layout_constraintTop_toTopOf="@+id/tls" />

    <androidx.appcompat.widget.AppCompatButton
        android:background="@drawable/bg"
        android:elevation="10dp"
        android:id="@+id/tpl"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_width="wrap_content"
        android:paddingBottom="15dp"
        android:paddingLeft="10dp"
        android:paddingRight="80dp"
        android:paddingTop="15dp"
        android:text="TPL"

        android:textAlignment="textStart"
        android:textAllCaps="true"
        android:textColor="@color/gnt_black"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/upc" />

    <TextView
        android:background="@drawable/bgg"
        android:elevation="10dp"
        android:id="@+id/textView4"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:paddingBottom="2dp"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:paddingTop="2dp"
        android:text="Developing"
        android:textAlignment="textStart"
        android:textColor="@color/gnt_black"
        android:textSize="8sp"
        app:layout_constraintBottom_toBottomOf="@+id/tpl"
        app:layout_constraintEnd_toEndOf="@+id/tpl"
        app:layout_constraintTop_toTopOf="@+id/tpl" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/upc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"

        android:background="@drawable/bg"
        android:elevation="10dp"
        android:focusable="true"
        android:paddingLeft="10dp"
        android:paddingTop="15dp"
        android:paddingRight="80dp"
        android:paddingBottom="15dp"
        android:text="UPD"
        android:textAlignment="textStart"
        android:textAllCaps="true"
        android:textColor="@color/gnt_black"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/protocolselect" />

    <TextView
        android:background="@drawable/bgg"
        android:elevation="10dp"
        android:id="@+id/textView2"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:paddingBottom="2dp"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:paddingTop="2dp"
        android:text="Developing"
        android:textAlignment="textStart"
        android:textColor="@color/gnt_black"
        android:textSize="8sp"
        app:layout_constraintBottom_toBottomOf="@+id/upc"
        app:layout_constraintEnd_toEndOf="@+id/upc"
        app:layout_constraintTop_toTopOf="@+id/upc" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:background="@drawable/bg"
        android:elevation="10dp"
        android:paddingLeft="45dp"
        android:paddingTop="15dp"
        android:paddingRight="45dp"
        android:paddingBottom="15dp"
        android:text="TCP"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/gnt_black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/protocolselect" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/view2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@drawable/bg"
        android:elevation="10dp"
        android:paddingLeft="45dp"
        android:paddingTop="15dp"
        android:paddingRight="45dp"
        android:paddingBottom="15dp"
        android:text="FTP"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/gnt_black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@drawable/bg"
        android:elevation="10dp"
        android:paddingLeft="45dp"
        android:paddingTop="15dp"
        android:paddingRight="45dp"
        android:paddingBottom="15dp"
        android:text="TCP+"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/gnt_black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="205dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:background="@drawable/bg"
        android:elevation="10dp"
        android:id="@+id/textView8"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_width="0dp"
        android:paddingBottom="15dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingTop="15dp"
        android:text="WS+"
    android:textAlignment="textStart"
        android:textColor="@color/gnt_black"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintStart_toStartOf="@+id/tls"
        app:layout_constraintTop_toTopOf="@+id/guideline3" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="272dp" />


    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
       android:layout_marginTop="20dp"
android:layout_marginLeft="36dp"
        android:layout_marginRight="36dp"
        app:cardBackgroundColor="@color/white"
        app:cardCornerRadius="20dp"
        app:cardElevation="3dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnHttp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/btn_bg"
                android:elevation="10dp"
                android:textColor="@color/gnt_black"
                android:paddingStart="20dp"
                android:text="HTTP"
                android:textAlignment="textStart" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/gray" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnHttpPlus"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/btn_bg"
                android:textColor="@color/gnt_black"

                android:elevation="10dp"
                android:paddingStart="20dp"
                android:text="HTTP+"
                android:textAlignment="textStart" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginBottom="5dp"
                android:background="@color/gray" />

            <TextView
                android:textColor="@color/gnt_gray"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:text="@string/pasteText" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <EditText
                    android:textColor="@color/gnt_gray"
                    android:id="@+id/etWebLink"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginEnd="94dp"
                    android:layout_marginRight="10dp"
                    android:background="@null"
                    android:hint="Paste a web link you can access"
                    android:maxWidth="200dp"
                    android:maxLines="2"
                    android:textSize="12sp" />

                <androidx.appcompat.widget.AppCompatButton
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="5dp"
                    android:layout_height="30dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="250dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_background"
                    android:gravity="center"
                    android:textColor="@color/white"

                    android:text="Apply"
                    android:textAlignment="center"
                    android:textSize="10sp" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="15dp"
                android:background="@color/gray" />

        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="32dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        app:cardBackgroundColor="@color/white"
        app:cardCornerRadius="20dp"
        app:cardElevation="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnTlsPlus"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/btn_bg"
                android:elevation="10dp"
                android:paddingStart="20dp"
                android:text="TLS+"
                android:textColor="@color/gnt_black"
                android:textAlignment="textStart" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginBottom="5dp"
                android:background="@color/gray" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="18dp"
                android:padding="8dp"
                android:textColor="@color/gnt_gray"
                android:text="Select a domain to improve connection" />

            <RelativeLayout
                android:id="@+id/btnSelectDomain"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="18dp"
                    android:drawableEnd="@drawable/ic_baseline_keyboard_arrow_right_24"
                    android:drawableTint="#686666"
                    android:padding="8dp"
                    android:text="Default"
                    android:textColor="@color/gnt_gray" />

            </RelativeLayout>


        </LinearLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>